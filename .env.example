# PostgreSQL
POSTGRES_DB=osm
POSTGRES_USER=osm
POSTGRES_PASSWORD=osm
PGPORT=5432

# Where to import OSM from (pick a region PBF)
# e.g., https://download.geofabrik.de/europe/germany/bayern/oberbayern-latest.osm.pbf
PBF_URL=https://download.geofabrik.de/europe/germany/bayern/oberbayern-latest.osm.pbf

# Spatial reference used for geometry storage (meters-friendly 3857 by default)
TARGET_SRID=3857